    dplyr (>= 1.0.10),
    jsonlite(>= 1.8.4),
    magrittr (>= 2.0.3),
    tibble (>= 3.1.8),
    tidyr (>= 1.3.0),
    rvest (>= 1.0.3)
Depends:
    furrr (>= 0.3.1),
    cpsR (>= 0.7.0),
    censusapi (>= 0.8.0),
    pbapply (>= 1.7-2)
RoxygenNote: 7.2.3
